{
  "name": "admin-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 5174",
    "build": "tsc -b && vite build",
    "preview": "vite preview --port 5174",
    "typecheck": "tsc -b --noEmit",
    "lint": "eslint .",
    "test:accept": "playwright test",
    "test:accept:ui": "playwright test --ui",
    "test:accept:debug": "playwright test --debug",
    "test:accept:report": "playwright show-report",
    "test:accept:setup": "cd ../api-server && npm run db:e2e:reset",
    "test:accept:parallel": "playwright test --workers=4",
    "test:accept:fast": "playwright test --workers=4 --retries=0",
    "test:accept:shard": "playwright test",
    "test:accept:workers": "playwright test --workers",
    "openapi:gen": "openapi-typescript http://localhost:4000/openapi.json -o src/types/openapi.d.ts"
  },
  "dependencies": {
    "@ai-sdk/react": "^2.0.72",
    "@emotion/react": "^11.14.0",
    "@mantine/core": "^8.3.2",
    "@mantine/dates": "^8.3.2",
    "@mantine/hooks": "^8.3.2",
    "@mantine/notifications": "^8.3.2",
    "@tabler/icons-react": "^3.35.0",
    "ai": "^5.0.72",
    "dayjs": "^1.11.18",
    "html5-qrcode": "^2.3.8",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.9.2",
    "recharts": "^3.2.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@playwright/test": "^1.49.1",
    "@tailwindcss/vite": "^4.1.13",
    "@types/node": "^24.7.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "openapi-typescript": "^7.9.1",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.7"
  }
}
